@model IEnumerable<IT_News.Models.ApplicationUser>

@{
    ViewBag.Title = "Users";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Users</h2>
@foreach (var user in Model)
{
    <tr>
            
        <td>
            <p>@user.UserName</p></td>
        <td>
            <p>@Html.ActionLink("EditUser","EditUser", "Account", new {id = user.Id},null)</p></td>
        <td>
    </tr>
}
